{
    "manifest_version": 3,
    "name": "the cutest chrome extension ever",
    "version": "1.0",
    "description": "add the cutest stuff to websites",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
      },
    "content_scripts": [
    {
        "js": ["scripts/p5.js", "scripts/content.js"],
        "matches": [
        "<all_urls>"
        ]
    }
    ]
  }